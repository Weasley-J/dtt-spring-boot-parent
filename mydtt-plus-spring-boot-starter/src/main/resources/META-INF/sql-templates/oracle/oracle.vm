-- Table structure for DTT_MEMBER

DROP TABLE "C##DTT"."DTT_MEMBER";
CREATE TABLE "C##DTT"."DTT_MEMBER"
(
    "ID"                    NUMBER not null primary key,
    "OPEN_ID"               VARCHAR2(64),
    "NICKNAME"              VARCHAR2(32),
    "IS_ENABLE"             NUMBER        default 1,
    "BALANCE"               NUMBER        default 0.00,
    "BIRTHDAY"              DATE,
    "MEMBER_TYPE"           VARCHAR2(128) default 'ORDINARY',
    "STATUS"                NUMBER        default 3,
    "DELETED"               NUMBER        default 0,
    "REGISTRAR_DATE"        DATE,
    "ACCELERATE_BEGIN_TIME" DATE,
    "ACCELERATE_END_TIME"   DATE,
    "UPDATE_TIME"           TIMESTAMP(6) WITH LOCAL TIME ZONE
);

