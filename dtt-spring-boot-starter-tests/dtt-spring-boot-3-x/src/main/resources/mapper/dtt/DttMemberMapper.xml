<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.mapper.DttMemberMapper">
    <select id="selectById" resultType="com.example.domain.dtt.DttMember">
        select *
        from dtt_member dm
        where dm.id = #{id};
    </select>
</mapper>
